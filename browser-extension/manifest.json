{
    "name": "URL change detector",
    "description": "detect a URL change in a tab, and inject a script to the page!",
    "version": "1.0",
    "omnibox": { "keyword" : "go/" },
    "manifest_version": 2,
    "permissions": [
        "<all_urls>",
        "storage",
        "tabs",
        "unlimitedStorage",
        "webRequest",
        "webRequestBlocking"
    ],

    "background": {
        "service_worker": "background.js"
    }
}